{
    "name": "webeweb/smsmode-library",
    "type": "library",
    "description": "Integrate sMsmode API with your projects",
    "keywords": ["webeweb", "library", "sMsmode", "API", "SMS"],
    "homepage": "http://github.com/webeweb/smsmode-library",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "http://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "webeweb/core-library": "^4.3|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*|^7.0"
    },
    "support": {
        "issues": "http://github.com/webeweb/smsmode-library/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\SMSMode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\SMSMode\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
